<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="CONSOLE" target="SYSTEM_OUT">
			<PatternLayout pattern="%4p %d{HH:mm:ss,SSS} %C - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="ERROR">
			<AppenderRef ref="CONSOLE"/>
		</Root>
    	<Logger name="log4j.logger.com.fourelementscapital.scheduler" level="ERROR" additivity="false">
			<Appender-ref ref="CONSOLE" />
    	</Logger>	
    	<Logger name="log4j.logger.com.fourelementscapital.config.Config" level="ERROR" additivity="false">
			<Appender-ref ref="CONSOLE" />
    	</Logger>	
    	<Logger name="log4j.logger.com.fourelementscapital.scheduler.common.SchedulerMgmt" level="ERROR" additivity="false">
			<Appender-ref ref="CONSOLE" />
    	</Logger>	
    	<Logger name="log4j.logger.com.fourelementscapital.client.LoginMgmt" level="ERROR" additivity="false">
			<Appender-ref ref="CONSOLE" />
    	</Logger>			
    	<Logger name="log4j.logger.com.fourelementscapital.loadbalance.hsqldb" level="ERROR" additivity="false">
			<Appender-ref ref="CONSOLE" />
    	</Logger>	
    	<Logger name="log4j.logger.org.quartz" level="ERROR" additivity="false">
			<Appender-ref ref="CONSOLE" />
    	</Logger>	
    	<Logger name="log4j.logger.org.directwebremoting" level="ERROR" additivity="false">
			<Appender-ref ref="CONSOLE" />
    	</Logger>	
	</Loggers>
</Configuration>