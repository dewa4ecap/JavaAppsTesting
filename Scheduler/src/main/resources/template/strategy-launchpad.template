    <script>
        function getCookie(cname) {
            var name = cname + "=";
            var decodedCookie = decodeURIComponent(document.cookie);
            var ca = decodedCookie.split(';');
            for(var i = 0; i <ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        function getBackTest(id) {
            $.ajax({
                url: "https://dev-qlib.alphien.com/scheduler/api/v1.2/action/launchpad/show/backtest/taskid/"+id,
                type: "POST",
                data: {
                    'ALUSER' : 'CURRENT_USER'
                    'ALSID' : 'CURRENT_TOKEN'
                },
                success: function(data) {
                    var tos = JSON.stringify(data);
                    var msg = JSON.parse(tos).data;
                    var check = JSON.parse(tos).message;
        		    var backRes = "";
                        for(var i in msg) {
                            // key : i, value : msg[i]
                            backRes = msg[i];
                        }

            		/*if (backRes=="Empty") {
			            if (JSON.parse(tos).message == "Finished") {  
                            //no backtest result	
                        } else {
                            setTimeout(getBackTest(id), 1000);
            			}
            		} else {
                        document.getElementById("iframe_backtest").src = "https://dev-qlib.alphien.com/scheduler/"+backRes;
		            }*/
		            
		            if (check == "Processing") {
                        setTimeout(getBackTest(id), 1000);
                    } else {
                    	if (backRes!="Empty") {
                    		document.getElementById("iframe_backtest").src = "https://beta.alphien.com/scheduler/"+backRes;
                    	}

                    }
		            
                },
                error: error=>(console.log(error))
            });
        }

        function getConsole(id,hit) {
            $.ajax({
                url: "https://dev-qlib.alphien.com/scheduler/api/v1.2/action/launchpad/show/console/taskid/"+id,
                type: "POST",
                data: {
                    'ALUSER' : 'CURRENT_USER'
                    'ALSID' : 'CURRENT_TOKEN'
                },
                success: function(data) {
                    var tos = JSON.stringify(data);
                    var msg = JSON.parse(tos).data;

            		if (msg==null) {
                                    output_console.setValue("Please wait for the result ...... " + hit , 1);
                                    setTimeout(getConsole(id,hit+1), 1000);
            		} else {
                        document.getElementById("btn_console").click();

		                var consoleMsg = "";
			            for(var i in msg) {
                            // key : i, value : msg[i]
                           	consoleMsg = msg[i];
                        }

			            if (consoleMsg=="" || consoleMsg==null) {
				            output_console.setValue("Please wait for the result ...... " + hit , 1);
                            setTimeout(getConsole(id,hit+1), 1000);
                        } else {
                            output_console.setValue(JSON.parse(consoleMsg).consoleoutput, 1);
                            getBackTest(id);
			            }
                    } 
                },
                error: error=>(console.log(error))
            });
        }

        function fn_asyncWeb(process) {

            var submitAccess = true;

            if (process==1) {
                var countDesc = document.getElementById("submit_desc").value;
                if (countDesc.length<100) {
                    submitAccess = false;
                }
            }
            
            if (!submitAccess) {
                alert("Description length should be 100 characters or above. \n Current length only "+ countDesc.length +" characters. Please try again.");
            }

            if ((editor.getValue()!="") && submitAccess) {

                output_console.setValue("Please wait for the result ...", 1);
                
                //clear backtest result
                document.getElementById("iframe_backtest").src = "";
                document.getElementById('btn_console').click();
                
                var script_description = "";
                
                //get description if any
                if (process == 1) {
                    script_description = document.getElementById("submit_desc").value;
                    document.getElementById("submit_desc").value="";
                    document.getElementById("myModal").style.display = "none";
                }
                
                var counter = 0;
                
                document.cookie = "user_script="+editor.getValue();

                $.ajax({
                    url: "https://dev-qlib.alphien.com/scheduler/api/v1.2/action/launchpad/run/script",
                    type: "POST",
                    data: {
                        'script' : editor.getValue()
                        'ALUSER' : 'CURRENT_USER'
                        'ALSID' : 'CURRENT_TOKEN'
                    },
                    success: function(res) {
						
            			var tos = JSON.stringify(res);
                        var msg = JSON.parse(tos).data;
                        var value = "";

                        for(var i in msg) {
                            // key : i, value : msg[i]
                            value = msg[i];
                        }
                        getConsole(value,0);
                    },
                    error: error=>(console.log(error))
                });

            } else {
                output_console.setValue("You can not run / submit empty Strategy !", 1);
            }
        }
    </script>

