    <script>
        function changeThemeAce(theme){
            var container_out = $("#container-launchpad-out-id");
            var btn_dark = $("#btn_change_theme_dark");
            var btn_light = $("#btn_change_theme_light");
            if(theme == "dark"){
                btn_dark.hide();
                btn_light.show();

                container_out.removeClass("cont_light_style");
                container_out.addClass("cont_dark_style");
                
                editor.setTheme("ace/theme/merbivore");
                output_console.setTheme("ace/theme/merbivore");
            }else{
                btn_light.hide();
                btn_dark.show();

                container_out.removeClass("cont_dark_style");
                container_out.addClass("cont_light_style");
                
                editor.setTheme("ace/theme/dreamweaver");
                output_console.setTheme("ace/theme/dreamweaver");
            }
        }
        
        $(document).keydown(function(e){
            if(e.ctrlKey && e.keyCode == 13){
                fn_asyncWeb(0);
            }
        })
