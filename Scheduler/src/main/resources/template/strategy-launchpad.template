                                    if(getCookie("user_console_output")!="") {
                                        output_console.getSession().setValue(getCookie("user_console_output"));
                                    }
                                    if(getCookie("user_backtest_output")!="") {
                                        document.getElementById("iframe_backtest").src = getCookie("user_console_output");
                                    }


                                    function openOutput(evt, tabName) {
                                        var i, tabcontent, tablinks;
                                        tabcontent = document.getElementsByClassName("tabcontent");
                                        for (i = 0; i < tabcontent.length; i++) {
                                            tabcontent[i].style.display = "none";
                                        }
                                        tablinks = document.getElementsByClassName("tablinks");
                                        for (i = 0; i < tablinks.length; i++) {
                                            tablinks[i].className = tablinks[i].className.replace(" active", "");
                                        }
                                        document.getElementById(tabName).style.display = "block";
                                        evt.currentTarget.className += " active";
                                    }

                                    var modal = document.getElementById("myModal");
                                    var span = document.getElementsByClassName("close");
                                    if(span.length > 0){
                                        span[0].onclick = function() {
                                            modal.style.display = "none";
                                        }    
                                    }
                                    
                                    window.onclick = function(event) {
                                        if (event.target == modal) {
                                            modal.style.display = "none";
                                        }
                                    }
                                    function displayBox(){
                                        if (editor.getValue()!="") {
                                            modal.style.display = "block";
                                        } else {
                                            output_console.setValue("You can not run / submit empty Strategy !", 1);        
                                        }
                                    }
                                    function cancel_invite(){
                                        modal.style.display = "none";
                                    }

                                    // riyan
                                    function disableScroll(){
                                        $("html, body").animate({scrollTop: "0px"}, 0);
                                        var h_window = $(window).height();
                                        var w_window = $(window).width();
                                        var content_cont = h_window - 195;
                                        var content_editor = h_window - 225;

                                        setTimeout(function(){
                                            var elmeditor = $("#editor");
                                            var elmconsole = $("#sl_console");

                                            var elmscreoll = $(".ace_scrollbar");
                                            var elmiframeback = $("#iframe_backtest");

                                            var elmresize = $(".ace_content");
                                            var elmcontainer = $(".cont_editor_resize");
                                            if(elmeditor.length && elmresize.length && elmcontainer.length && elmconsole.length){
                                                // resize height
                                                elmcontainer.css({"height":content_cont+"px"});
                                                elmeditor.css({"height":content_editor+"px"});
                                                elmresize.css({"height":content_editor+"px"});
                                                elmconsole.css({"height":content_editor+"px"});
                                                elmconsole.css({"min-height":content_editor+"px"});
                                                elmscreoll.css({"min-height":content_editor+"px"});
                                                elmiframeback.css({"height":content_editor+"px"});
                                            }
                                        }, 1500);
                                        if(w_window > 991){
                                            $("html").attr("style", "overflow-y: hidden;");
                                        }else{
                                            $("html").attr("style", "overflow-y: auto;");
                                        }
                                    }
                                    disableScroll();
                                    $(window).resize(function(){
                                        disableScroll();
                                    });
                                </script>
                            </div><!-- .entry-content -->
                        </article><!-- #post-## -->
                    </main><!-- #main -->
                </div><!-- #primary -->
            </div><!-- #content -->
        </div> <!-- .container -->

